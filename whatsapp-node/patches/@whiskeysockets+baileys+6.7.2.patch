-					const index = mutation.syncAction.index?.toString()
-					mutationMap[index!] = mutation
-				}
-				: (() => { }),
-			true
-		)
-
-		newState.hash = decodeResult.hash
-		newState.indexValueMap = decodeResult.indexValueMap
-
-		if(validateMacs) {
